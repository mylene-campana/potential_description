<launch>
  <param name="robot_description" command="cat $(find potential_description)/urdf/object.urdf"/>
  <param name="obstacle_description" command="cat $(find potential_description)/urdf/cylinder_obstacle.urdf"/>
  
  <node name="odom_link_broadcaster"
    pkg="tf" type="static_transform_publisher"  
    args="0 0 0 0 0 0 /map /odom 200"/>
    
  <node name="robot_state_publisher"
	pkg="robot_state_publisher"
	type="state_publisher"
	respawn="true">
    <param name="tf_prefix" value="" />
  </node>
  <node name="rviz"
	pkg="rviz"
	type="rviz"
	respawn="false">
  </node>
</launch>
